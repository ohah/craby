{
  "name": "craby-test",
  "private": true,
  "description": "Craby test module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "clean": "craby clean",
    "build": "craby build && yarn build:js",
    "build:js": "tsdown"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "author": "leegeunhyeok <dev.ghlee@gmail.com> (https://github.com/leegeunhyeok)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/craby/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/craby#readme",
  "dependencies": {
    "craby-modules": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@react-native-community/cli-types": "^20.0.2",
    "@types/react": "^19.0.0",
    "crabygen": "workspace:*",
    "react": "19.1.1",
    "react-native": "0.81.4",
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
