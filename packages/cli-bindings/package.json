{
  "name": "@craby/cli-bindings",
  "version": "0.1.0-beta.4",
  "description": "N-API binding package for Craby CLI",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "scripts": {
    "dev": "yarn build && ./bin.js",
    "build": "./scripts/build.sh",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "test:bindings": "vitest --run"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/craby.git",
    "directory": "packages/cli-bindings"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.1.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "napi": {
    "binaryName": "cli-bindings",
    "esm": true,
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
