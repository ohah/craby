{
  "name": "@craby/devkit",
  "version": "0.1.0-beta.8",
  "description": "Development kit for Craby",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "build": "tsdown"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/craby.git",
    "directory": "packages/craby-modules"
  },
  "author": "leegeunhyeok <dev.ghlee@gmail.com> (https://github.com/leegeunhyeok)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/craby/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/craby#readme",
  "dependencies": {
    "oxc-resolver": "^11.11.1"
  },
  "devDependencies": {
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.10.3"
}
