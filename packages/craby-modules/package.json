{
  "name": "craby-modules",
  "version": "0.1.0-rc.2",
  "description": "TypeScript definitions and runtime for Craby modules",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "build": "tsdown"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/craby.git",
    "directory": "packages/craby-modules"
  },
  "author": "leegeunhyeok <dev.ghlee@gmail.com> (https://github.com/leegeunhyeok)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/craby/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/craby#readme",
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "react": "^19.1.1",
    "react-native": "^0.81.4",
    "tsdown": "^0.15.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react-native": ">=0.76.0"
  },
  "packageManager": "yarn@4.10.3"
}
